<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
      <dl class="dl-horizontal">
        <% @my_likes.each do |mylike| %>
        <tr>
          <td><img src= "<%= mylike.photo.image %>" ></td>
          <td><%= mylike.photo.caption %></td>
        </tr>
        <% end %>
      </dl>
  </div>
</div>
